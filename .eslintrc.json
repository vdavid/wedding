{
    "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "error",
        "no-console": "warn",
        "quotes": ["error", "single"],
        "jsx-quotes": ["error", "prefer-double"],
        "indent": ["error", 4],
        "semi": ["error", "never"],
        "comma-dangle": ["error", "always-multiline"]
    }
}
